{
  "name": "@giosg-design-system/icons",
  "private": false,
  "description": "svg based react icon components",
  "version": "2.0.0",
  "source": "./src/components/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/module.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "svg": "svgr -- src/svg && eslint src/components --fix",
    "build": "pnpm run svg && tsc --noEmit && parcel build --no-cache",
    "dev": "parcel watch --port $(../../scripts/getPort.sh)",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "package": "rm -rf *.tgz && pnpm pack"
  },
  "peerDependencies": {
    "react": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@giosg-design-system/foundations": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@svgr/cli": "^8.1.0",
    "eslint": "^8.54.0",
    "parcel": "2.12.0",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

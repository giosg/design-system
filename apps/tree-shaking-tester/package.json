  },
  "dependencies": {
    "@giosg-design-system/icons": "workspace:*",
    "react": ">=18"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "parcel": "2.12.0",
    "react": "^18.2.0",
    "typescript": "^5.3.2"
  }
}

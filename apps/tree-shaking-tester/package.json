{
  "name": "@repo/tree-shaking-tester",
  "version": "0.0.7",
  "private": true,
  "source": "./src/index.tsx",
  "main": "./dist/index.js",
  "module": "./dist/module.mjs",
  "targets": {
    "module": {
      "optimize": true,
      "includeNodeModules": [
        "@giosg-design-system/icons"
      ]
